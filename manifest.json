{
  "name": "ADD Assist",
  "description": "Make a web site more accessible to those who suffer from attention deficit disorder.",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "/images/AdD16.png",
      "32": "/images/AdD32.png",
      "128": "/images/AdD128.png"
    }
  },
  "icons": {
    "16": "/images/AdD16.png",
    "32": "/images/AdD32.png",
    "128": "/images/AdD128.png"
  },
  "options_page": "options.html"
}
